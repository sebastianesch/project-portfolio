{
    "configurations": [
        {
            "command": "cds run --with-mocks --in-memory?",
            "name": "cds run",
            "request": "launch",
            "type": "node-terminal",
            "skipFiles": [
                "<node_internals>/**"
            ]
        },
        {
            "name": "Start projectsapp Mock",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}/app/projectsapp",
            "runtimeExecutable": "npx",
            "windows": {
                "runtimeExecutable": "npx.cmd"
            },
            "runtimeArgs": [
                "fiori",
                "run"
            ],
            "args": [
                "--config",
                "./ui5-mock.yaml",
                "--open",
                "test/flpSandbox.html#projectportfolioprojectsapp-tile"
            ],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "outputCapture": "std",
            "port": 9229,
            "env": {
                "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false",
                "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/project-portfolio/app/projectsapp\"}"
            }
        },
        {
            "name": "Start projectsapp Local",
            "type": "node",
            "request": "launch",
            "cwd": "${workspaceFolder}/app/projectsapp",
            "runtimeExecutable": "npx",
            "windows": {
                "runtimeExecutable": "npx.cmd"
            },
            "runtimeArgs": [
                "fiori",
                "run"
            ],
            "args": [
                "--config",
                "./ui5-local.yaml",
                "--open",
                "test/flpSandbox.html#projectportfolioprojectsapp-tile"
            ],
            "console": "internalConsole",
            "internalConsoleOptions": "openOnSessionStart",
            "outputCapture": "std",
            "port": 9229,
            "env": {
                "FIORI_TOOLS_URL_PARAMS": "sap-ui-xx-viewCache=false",
                "run.config": "{\"handlerId\":\"fiori_tools\",\"runnableId\":\"/home/user/projects/project-portfolio/app/projectsapp\"}"
            }
        }
    ],
    "compounds": [],
    "version": "0.2.0"
}